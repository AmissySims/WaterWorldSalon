<Page x:Class="Courier.Pages.MainMainPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Courier.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MainMainPage">

    <Grid>
        <StackPanel Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
            <Button Content="Заказы" Style="{StaticResource ImportantBtn}" x:Name="OrderdBtn" Click="OrderdBtn_Click" />
            <Button Content="Аккаунт" Style="{StaticResource ImportantBtn}" x:Name="AccountBtn" Click="AccountBtn_Click" />
        </StackPanel>
    </Grid>
</Page>
